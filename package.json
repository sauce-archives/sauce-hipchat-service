  "scripts": {
    "docker:build": "docker build -t gcr.io/my-kubernetes-1269/sauce-hipchat-service:v$(node -e \"console.log(require('./package.json').version)\") .",
    "docker:push": "gcloud docker -- push gcr.io/my-kubernetes-1269/sauce-hipchat-service:v$(node -e \"console.log(require('./package.json').version)\")",
    "docker:deploy": "kubectl rolling-update --container sauce-hipchat-svc sauce-hipchat-ctrl --image-pull-policy Always --image=gcr.io/my-kubernetes-1269/sauce-hipchat-service:v$(node -e \"console.log(require('./package.json').version)\")",
    "build": "./bin/build.sh",
    "heroku-postbuild": "./bin/build.sh",
    "dev": "nodemon -w app.js -w lib -w atlassian-connect.json -w config.json -w routes/index.js app.js",
    "start": "node app.js",
    "test": "TZ=GMT NODE_ENV=test mocha",
    "postversion": "git push --tags && conventional-github-releaser -p angular"
  },
